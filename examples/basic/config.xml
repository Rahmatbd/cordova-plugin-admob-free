<?xml version='1.0' encoding='utf-8'?>
<widget id="hello.admob" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Hello AdMob</name>
    <description>
        A sample cordova-plugin-admob-free application.
    </description>
    <author email="ratson@users.noreply.github.com" href="https://github.com/ratson/cordova-plugin-admob-free">
        Ratson
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <preference name="android-build-tool" value="gradle" />
         <preference name="phonegap-version" value="cli-9.0.0" />
         <preference name="android-targetSdkVersion" value="28" />
         <preference name="android-minSdkVersion" value="16" />
    <plugin name="cordova-plugin-whitelist" spec="^source="npm"" />
    <plugin name="cordova-plugin-admob-free" spec="source="npm"" />
    <engine name="android" spec="7.1.4" />
    <engine name="ios" spec="~4.5.5" />
</widget>
